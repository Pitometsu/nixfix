(name ppx_protocol_conv)
(version 5.0.0)
(library
 (name ppx_protocol_conv)
 (kind ppx_deriver)
 (synopsis "ppx to derive (de)serializers of a type")
 (archives (byte ppx_protocol_conv.cma) (native ppx_protocol_conv.cmxa))
 (plugins (byte ppx_protocol_conv.cma) (native ppx_protocol_conv.cmxs))
 (foreign_archives (native ppx_protocol_conv.a))
 (requires base ppxlib)
 (main_module_name Ppx_protocol_conv)
 (modes byte native)
 (modules
  (main_module_name Ppx_protocol_conv)
  (modules
   ((name Ppx_protocol_conv)
    (obj_name ppx_protocol_conv)
    (visibility public)
    (impl)
    (intf)))
  (wrapped true)))
