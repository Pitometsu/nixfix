    (obj_name stdint__Int_wrapper)
    (visibility public)
    (impl)
    (intf))
   ((name Stdint) (obj_name stdint) (visibility public) (impl) (intf))
   ((name Str_conv)
    (obj_name stdint__Str_conv)
    (visibility public)
    (impl)
    (intf)))
  (wrapped true)))
