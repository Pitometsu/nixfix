(lang dune 1.10)
(name stdint)
(library
 (name stdint)
 (kind normal)
 (synopsis "Standard integer types for OCaml")
 (archives (byte stdint.cma) (native stdint.cmxa))
 (plugins (byte stdint.cma) (native stdint.cmxs))
 (foreign_objects
  float_conv.o
