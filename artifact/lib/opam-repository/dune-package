   ((name OpamLocal) (obj_name opamLocal) (visibility public) (impl) (intf))
   ((name OpamRepository)
    (obj_name opamRepository)
    (visibility public)
    (impl)
    (intf))
   ((name OpamRepositoryBackend)
    (obj_name opamRepositoryBackend)
    (visibility public)
    (impl)
    (intf))
   ((name OpamRepositoryConfig)
    (obj_name opamRepositoryConfig)
    (visibility public)
    (impl)
    (intf))
   ((name OpamRepositoryPath)
    (obj_name opamRepositoryPath)
    (visibility public)
    (impl)
    (intf))
   ((name OpamVCS) (obj_name opamVCS) (visibility public) (impl) (intf)))
  (wrapped false)))
