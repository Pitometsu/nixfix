(lang dune 1.10)
(name opam-repository)
(version 2.0.4)
(library
 (name opam-repository)
 (kind normal)
 (synopsis "OCaml Package Manager remote repository handling library")
 (archives (byte opam_repository.cma) (native opam_repository.cmxa))
 (plugins (byte opam_repository.cma) (native opam_repository.cmxs))
 (foreign_archives (native opam_repository.a))
 (requires opam-format)
 (modes byte native)
 (modules
  (modules
   ((name OpamDarcs) (obj_name opamDarcs) (visibility public) (impl) (intf))
   ((name OpamDownload)
    (obj_name opamDownload)
    (visibility public)
